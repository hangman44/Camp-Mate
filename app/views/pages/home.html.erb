<div class="center jumbotron">
  <h1>CAMPMATE</h1>
  <%= link_to "Sign up now", signup_path, class: "btn btn-danger btn-lg btn-xlarge" %>
</div>

   <h2 class="camping"> The 3 Best Places to Camp</h2>
  
   <h2 class="places"> 1- Huntsville State Park</h2>
        
<div id="carousel-a" class="carousel slide carousel-sync" data-ride="carousel" data-pause="false">
  <div class="carousel-inner">
    <div class="item active">
      <%= image_tag "Huntsville-_MG_5878.jpg" %>
    </div>
    <div class="item">
      <%= image_tag "Huntsville-_MG_5904.jpg" %>
    </div>
    <div class="item">
      <%= image_tag "Huntsville_066.jpg" %>
    </div>
  </div>

  <a class="left carousel-control" href="#carousel-a" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" href="#carousel-a" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div>
 
  <h2 class="places"> 2- Ludington State Park</h2>
  
